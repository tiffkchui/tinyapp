<header>
  <nav class="navbar navbar-expand-md navbar-dark bg-info">
    <a class="navbar-brand" href="/urls">TinyApp</a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup"
      aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
      <div class="navbar-nav">
        <a class="nav-item nav-link" href="/urls">My URLs</a>
        <a class="nav-item nav-link" href="/urls/new">Create New URL</a>

      </div>
    </div>


    <nav>

      <nav>
        <ul>

          <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
            <ul class="nav navbar-nav navbar-right">
              <div class="navbar-nav">
                  <% if ( email !== '') { %>
                    <span class="navbar-text mr-2">Logged in as: <%= email %></span>
                    <form method="POST" action="/logout" class="form-inline my-2 my-lg-0">
                      <button type="submit" class="btn btn-outline-light my-2 my-sm-0">Log Out</button>
                    </form>
                  <% } else { %>
                    <li><a class="nav-item nav-link" href="/login">Login</a></li>
                    <li><a class="nav-item nav-link" href="/register">Register</a></li>
                  <% } %>
              </div>
            </ul>
      </nav>
</header>